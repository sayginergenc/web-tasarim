
    basla

        s1,s2,ort,vize,final


            yaz "saygı gir",
            oku s1
            yaz "2.sayıyı gir""
            oku s2
            yaz vize % gir
            oku vize
            final=vize-%100
            ort=vize*s1+final*s2 yada final tanımlama "%100-vize"
            yaz"sonuc"=ort
    eğer(ort>=50 ve final>=50)
        yaz"tebrikler gectin"
    degilse
        yaz"seneye gel"
    bitir 
    basla
    kilo, boy,beeden
    yaz "kılonu gir"
    oku kilo
    yaz "cm olaraak boy yaz"
    oku boy
    beden=kılo/(boy*0.0001)
    yaz"sonuc"+beden
    eger(beden<=19)
    yaz "zayıf"
    değilse eger(beden>=19 ve beden<=24)
    yaz "normal"
    değilse
    yaz"sısmansın ha"
                                        DERS 2
    BAŞLA
        sayı,toplam=0,x
sayac(x=1;`den "10a`kadar 1`er)
yaz"x+ sayı gir"
oku sayı
toplam=toplam+sayı,
sayacsonu
yaz sonuc="+toplam"
bitir
BAŞLA
    x,toplam=0
    sayac(x=1`den "100a`a kadar 1`er")
    toplam=toplam+x
    sayacsonu
    yaz"sonuc="+toplam
    bitir
    